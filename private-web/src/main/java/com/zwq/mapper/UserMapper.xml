<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zwq.dao.UserDao">
                    
    <select id="getUser" resultType="com.zwq.entity.UserModel" parameterType="java.lang.Integer">
	    SELECT
	        id,
	        name,
	        age
	    FROM
	        user_table
	    WHERE
	        id = #{id}
    </select>
    
    <insert id="saveUser" parameterType="com.zwq.entity.UserModel" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO user_table (name, age)
		VALUES
		    (#{name},#{age})
    </insert>
    
</mapper>
